$('.text-danger').remove()
<% if @errors.nil? %>
$("input:password").val('');
$('#notifications').html('<%= alert_bs "Change password success" %>')
<% else %>
  <% @errors.each do |key, value| %>
    $('#user_<%= key %>').after('<%= contextual t(".#{key}") + ' ' + value.last %>')
  <% end %>
<% end %>


