<div class="col-md-12 icon-menu">
  <div class="col-md-1">
    <%= image_tag 'no-image.jpg', class: 'img-thumbnail' %>
  </div>
  <div class="col-md-5">
    <h4><%= t('users.title') %></h4>
  </div>
  <div class="col-md-3 private-dhk text-right">
    <h5>
      <a class="<%= 'glyphicon glyphicon-star-empty font-size-15' if session[:profile_tab] == :info %>"></a><%= link_to t('.info'), user_path, class: "#{session[:profile_tab] == :info ? 'booking_tab' : ''}" %>
    </h5>
  </div>
  <div class="col-md-3 private-dhk">
    <h5>
      <a class="<%= 'glyphicon glyphicon-star-empty font-size-15' if session[:profile_tab] == :e_walet %>"></a><%= link_to t('.e_wallet'), e_walet_user_path, class: "#{session[:profile_tab] == :e_walet ? 'booking_tab' : ''}" %>
    </h5>
  </div>
</div>


<div class="panel-group col-md-12 margin-top-20" id="accordion">
  <div class="panel panel-default">
    <div class="panel-heading">
      <h4 class="panel-title">
        <a data-toggle="collapse" data-parent="#accordion" href="#my_bookings">
          <%= t('booking.my_bookings') %>
        </a>
      </h4>
    </div>
    <div id="my_bookings" class="panel-collapse collapse">
      <div class="panel-body load-content-booking">
        <%= render 'my_booking' %>
      </div>
    </div>
  </div>
  <div class="panel panel-default">
    <div class="panel-heading">
      <h4 class="panel-title">
        <a data-toggle="collapse" data-parent="#accordion" href="#my_privileges">
          <%= t('privileges.my_privileges') %>
        </a>
      </h4>
    </div>
    <div id="my_privileges" class="panel-collapse collapse">
      <div class="panel-body load-content-privileges">
        <%= render 'my_privileges' %>
      </div>
    </div>
  </div>
</div>

